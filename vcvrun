for user in $(cat candidates); do (cat vcvstatus | grep ${user} >/dev/null) || (gh pr checks $(gh pr list | grep ${user} | awk '{print $1}') | grep 'build.pass' && echo ${user} passed with signature $(gh pr status | grep -A1 ${user} | sha256sum | awk '{print $1}')); done >>vcvstatus
git add candidates vcvstatus 
git commit -m "vcv update $(date +%s)"
git pull
git push
